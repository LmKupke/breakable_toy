<div class="large-4 medium-6 small-12 columns">
  <div class="image-wrapper overlay-fade-in animated fadeInLeft">
    <%= image_tag(venue.photo, alt: "#{venue.name} photo", class: "#{venue.name} photo") %>

    <div class="panel">
      <p class="subheader"><%= venue.rating %></p>
      <div class="image-overlay-content">
        <h4><%= link_to venue.name, venue_path(venue) %></h4>
      </div>
    </div>
  </div>
</div>
